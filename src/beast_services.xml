<package name="metastabayes" version="1.0.0">
  <depends on="BEAST.base" atleast="2.7.4"/>
  <depends on="BEAST.app" atleast="2.7.4"/>

  <service type="beast.base.core.BEASTInterface">
    <provider classname="metastabayes.likelihood.MetastabayesBeagleTreeLikelihood"/>
    <provider classname="metastabayes.substitutionmodel.MetastabayesGeneralMutationSubstitutionModel"/>
    <provider classname="metastabayes.substitutionmodel.MetastabayesGeneralTissueSubstitutionModel"/>
    <provider classname="metastabayes.substitutionmodel.MetastabayesGeneralTissueSubstitutionModelLogger"/>
    <provider classname="tidetree.evolution.datatype.EditData"/>
    <provider classname="tidetree.substitutionmodel.EditAndSilencingModel"/>
    <provider classname="tidetree.tree.StartingTree"/>
    <provider classname="tidetree.distributions.TreeLikelihoodWithEditWindow"/>
    <provider classname="tidetree.simulation.SimulatedAlignment"/>
  </service>
</package>
