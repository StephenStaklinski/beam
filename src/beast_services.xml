<package name="beam" version="1.0.0">
  <depends on="BEAST.base" atleast="2.7.4"/>
  <depends on="BEAST.app" atleast="2.7.4"/>

  <service type="beast.base.core.BEASTInterface">
    <provider classname="beam.likelihood.BeamBeagleTreeLikelihood"/>
    <provider classname="beam.likelihood.BeamAncestralStateBeagleTreeLikelihood"/>
    <provider classname="beam.tree.BeamStartingTreeFromNewick"/>
    <provider classname="beam.substitutionmodel.BeamGeneralMutationSubstitutionModel"/>
    <provider classname="beam.substitutionmodel.BeamGeneralTissueSubstitutionModel"/>
    <provider classname="beam.substitutionmodel.BeamGeneralTissueSubstitutionModelLogger"/>
    <provider classname="tidetree.evolution.datatype.EditData"/>
    <provider classname="tidetree.substitutionmodel.EditAndSilencingModel"/>
    <provider classname="tidetree.tree.StartingTree"/>
    <provider classname="tidetree.distributions.TreeLikelihoodWithEditWindow"/>
    <provider classname="tidetree.simulation.SimulatedAlignment"/>
  </service>
</package>
